{
  "manifest_version": 2,
  "name": "Tweet It",
  "description": "Tweet what you see on the Web!",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "tutter.png",
    "32": "tutter.png",
    "64": "tutter.png",
    "128": "tutter.png"
  },
  "permissions": [
    "contextMenus"
  ]
}